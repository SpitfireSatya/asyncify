{
  "allow-uncaught": false,
  "async-only": false,
  "bail": false,
  "check-leaks": true,
  "color": true,
  "delay": false,
  "diff": true,
  "exit": true,
  "forbid-only": true,
  "forbid-pending": true,
  "full-trace": true,
  "inline-diffs": true,
  "recursive": true,
  "reporter": "spec",
  "retries": 1,
  "slow": 30,
  "sort": false,
  "test": "**/*.spec.ts",
  "ignore": [
    "node_modules/**"
  ],
  "extension": ["ts"],
  "require": "ts-node/register",
  "timeout": 500,
  "trace-warnings": true,
  "ui": "bdd",
  "v8-stack-trace-limit": 100,
  "watch": false,
  "parallel": false
}